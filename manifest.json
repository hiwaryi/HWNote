{
  "manifest_version": 2,
  "name": "HWNote",
  "description": "This extension helps you doing search for you homework.",
  "version": "1.0",

  "permissions": [
    "tabs", "<all_urls>", "sessions"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "HWNote",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["js/eventPage.js"],
    "persistent": false
  }
}
